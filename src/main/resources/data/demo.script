CREATE MEMORY TABLE EMP2(EMPNO NUMERIC(4) NOT NULL PRIMARY KEY,ENAME VARCHAR(10),DEPTNUM NUMERIC(2))
CREATE MEMORY TABLE DEPT2(DEPTNO NUMERIC(2) NOT NULL PRIMARY KEY,DNAME VARCHAR(14))
CREATE MEMORY TABLE BOOK(ID NUMERIC(5) NOT NULL PRIMARY KEY,CATEGORY VARCHAR(10),TITLE VARCHAR(10))
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 60
SET SCHEMA PUBLIC
